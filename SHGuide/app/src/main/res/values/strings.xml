    <string name="nature_item1_name">Wildpark Eekholt</string>
    <string name="nature_item1_address">Eekholt 1 | Großenaspe, 24623 Bad Segeberg</string>
    <string name="nature_item1_description">Wildpark Eekholt is an excellent location to see the wildlife of Northern Gemany and has great excursions all year long that you can take part in.</string>
    <string name="nature_item1_number">+49 04327 9923</string>
    <string name="nature_item1_website">wildpark-eekholt.de</string>
    <string name="nature_item1_locationURL">https://www.google.com/maps/place/Eekholt+1,+24623+Gro%C3%9Fenaspe/@53.9534441,10.0321943,17z/data=!3m1!4b1!4m5!3m4!1s0x47b2371ec82d436d:0x1cf68c7f1742c01b!8m2!3d53.953441!4d10.034383</string>

    <string name="nature_item2_name">Sankt Peter Ording Beach</string>
    <string name="nature_item2_address">Sankt Peter-Ording, Schleswig-Holstein, Germany</string>
    <string name="nature_item2_description">Sankt Peter-Ording is a popular German seaside spa and a municipality in the district of Nordfriesland, in Schleswig-Holstein, Germany. It is the only German seaside resort that has a sulphur spring and thus terms itself "North Sea spa and sulphur spring"</string>
    <string name="nature_item2_number">+49 4863 9990</string>
    <string name="nature_item2_website">st-peter-ording.de</string>
    <string name="nature_item2_locationURL">https://www.google.com/maps/place/25826+Sankt+Peter-Ording/@54.324532,8.62275,13z/data=!4m5!3m4!1s0x47b465f127242de3:0x4248963c6582350!8m2!3d54.3048043!4d8.6522205</string>

    <string name="nature_item3_name">Steinzeitpark Dithmarschen</string>
    <string name="nature_item3_address">Suederstraße 47, 25767 Albersdorf, Schleswig-Holstein</string>
    <string name="nature_item3_description">Steinzeitpark Dithmarschen is an archaeological Open-Air-Museum that reconstructs history at real scale! It spans over 40 hectares!</string>
    <string name="nature_item3_number">+49 4835 971097</string>
    <string name="nature_item3_website">steinzeitpark-dithmarschen.de</string>
    <string name="nature_item3_locationURL">https://www.google.com/maps/place/Steinzeitpark+Dithmarschen/@54.1377944,9.286411,17z/data=!3m1!4b1!4m5!3m4!1s0x47b3938c57137b6b:0x2cf3ce686ab07a6f!8m2!3d54.1377944!4d9.2885997</string>

    <string name="nature_item4_name">Nord-Ostsee-Kanal</string>
    <string name="nature_item4_address">Kiel Canal, Brunsbüttel, Schleswig-Holstein, Germany</string>
    <string name="nature_item4_description">Connecting North Sea and the Baltic Sea, this canal spans a distance of 98km. All ferries that cross it can be used for free!</string>
    <string name="nature_item4_number">+49 4651 9870888</string>
    <string name="nature_item4_website">tag-nok.de/</string>
    <string name="nature_item4_locationURL">https://www.google.com/maps/place/Kiel+Canal/@54.1498808,9.6556588,13z/data=!4m5!3m4!1s0x47b3b9f3f6824b3b:0x8209ec730ede5728!8m2!3d54.1272272!4d9.6527628</string>

