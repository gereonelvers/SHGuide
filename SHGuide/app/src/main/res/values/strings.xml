    <string name="entertainment_item2_name">Arriba Erlebnisbad</string>
    <string name="entertainment_item2_address">Am Hallenbad 1, 22850 Norderstedt, Schleswig-Holstein</string>
    <string name="entertainment_item2_description">With 5 slides, multiple different pools and a huge sauna area, the Arriba is a prime location for everyone who loves water!</string>
